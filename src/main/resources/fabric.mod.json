{
  "schemaVersion": 1,
  "id": "spectrum",
  "version": "${version}",
  "name": "Spectrum",
  "description": "A full-feature minecraft magic mod about harnessing the powers of color",
  "authors": [
    "DaFuqs"
  ],
  "contact": {
    "homepage": "https://github.com/DaFuqs/",
    "sources": "https://github.com/DaFuqs/Spectrum"
  },
  "license": "",
  "icon": "assets/spectrum/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "de.dafuqs.spectrum.SpectrumCommon"
    ],
    "client": [
      "de.dafuqs.spectrum.SpectrumClient"
    ],
    "modmenu": [
      "de.dafuqs.spectrum.config.ModMenuConfig"
    ],
    "rei_plugins": [
      "de.dafuqs.spectrum.compat.REI.REIServerIntegration",
      "de.dafuqs.spectrum.compat.REI.REIClientIntegration"
    ],
    "cardinal-components": [
      "de.dafuqs.spectrum.azure_dike.AzureDikeProvider"
    ]
  },
  "accessWidener": "spectrum.accesswidener",
  "mixins": [
    "spectrum.mixins.json"
  ],
  "custom": {
    "cardinal-components": [
      "spectrum:azure_dike"
    ]
  },
  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": ">=0.42.1",
    "minecraft": ">=1.18.1",
    "java": ">=17",
    "cloth-config": ">=6.0.0",
    "patchouli": "*",
    "owo": ">=0.5.2",
    "trinkets": ">=3.2.1"
  },
  "suggests": {
    "colored_lights": ">=1.0.3",
    "advancements-enlarger": ">=0.2.4"
  },
  "breaks": {
    "Dashloader": "<= 3.0.x"
  }
}
